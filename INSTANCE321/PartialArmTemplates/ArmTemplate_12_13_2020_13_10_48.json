						"type": "nvarchar"
					},
					{
						"name": "Infant_mortality",
						"type": "nvarchar"
					},
					{
						"name": "GDP",
						"type": "nvarchar"
					},
					{
						"name": "Literacy",
						"type": "nvarchar"
					},
					{
						"name": "Phones",
						"type": "nvarchar"
					},
					{
						"name": "Arable",
						"type": "nvarchar"
