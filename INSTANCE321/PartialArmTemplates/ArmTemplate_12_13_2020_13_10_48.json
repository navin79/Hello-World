						"name": "Deathrate",
						"type": "nvarchar"
					},
					{
						"name": "Agriculture",
						"type": "nvarchar"
					},
					{
						"name": "Industry",
						"type": "nvarchar"
					},
					{
						"name": "Service",
						"type": "nvarchar"
					}
				],
