{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "INSTANCE321"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"InputBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'InputBlob'"
		},
		"cars_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'cars'"
		},
		"cars_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'cars_LS'"
		},
		"demo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'demo'"
		},
		"demostrg_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'demostrg'"
		},
		"newlink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'newlink'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "INSTANCE321_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"InputBlob_connectionString": {
						"value": "[parameters('InputBlob_connectionString')]"
					},
					"cars_connectionString": {
						"value": "[parameters('cars_connectionString')]"
					},
					"cars_LS_connectionString": {
						"value": "[parameters('cars_LS_connectionString')]"
					},
					"demo_connectionString": {
						"value": "[parameters('demo_connectionString')]"
					},
					"demostrg_connectionString": {
						"value": "[parameters('demostrg_connectionString')]"
					},
					"newlink_connectionString": {
						"value": "[parameters('newlink_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}